			"item": [
				{
					"name": "DO NOT USE - Get Token - Global (domain context)",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "7f3637e1-27a9-4446-b4b4-04dcac8dde27",
								"exec": [
									"pm.environment.set(\"token\", pm.response.headers.get(\"X-Subject-Token\"));",
									"pm.environment.set(\"token_timestamp\", new Date().getTime());",
									"pm.environment.set(\"token_date\", pm.response.headers.get(\"Date\"));",
									"",
									"var jsonData = pm.response.json();",
									"pm.environment.set(\"token_expires_at\", jsonData.token.expires_at);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/json;charset=utf8",
								"type": "text"
							}
						],
