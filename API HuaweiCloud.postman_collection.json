			"item": [
				{
					"name": "Get New Token - region specific (project context)",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "a8865603-e52f-4a59-9c56-0f06879c74bd",
								"exec": [
									"pm.environment.set(\"token\", pm.response.headers.get(\"X-Subject-Token\"));",
									"// pm.environment.set(\"token_timestamp\", new Date().getTime());",
									"// pm.environment.set(\"token_date\", pm.response.headers.get(\"Date\"));",
									"",
									"var jsonData = pm.response.json();",
									"console.log(jsonData);",
									"pm.environment.set(\"token_expires_at\", jsonData.token.expires_at);",
									"pm.environment.set(\"project_name\", jsonData.token.project.name);",
									"pm.environment.set(\"project_id\", jsonData.token.project.id);",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"type": "text",
								"value": "application/json;charset=utf8"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\r\n  \"auth\": {\r\n    \"identity\": {\r\n      \"methods\": [\r\n        \"password\"\r\n      ],\r\n      \"password\": {\r\n        \"user\": {\r\n          \"name\": \"{{user_name}}\",\r\n          \"password\": \"{{user_password}}\",\r\n          \"domain\": {\r\n            \"name\": \"{{account}}\"\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \"scope\": {\r\n      \"project\": {\r\n        \"name\": \"{{region}}\"\r\n      }\r\n    }\r\n  }\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "https://{{endpoint_iam}}/v3/auth/tokens",
							"protocol": "https",
							"host": [
								"{{endpoint_iam}}"
							],
							"path": [
								"v3",
								"auth",
								"tokens"
							]
						},
						"description": "Tokens obtained by calling this API using the global endpoint can be used for service access in all regions except Paris, Singapore, and Atlanta. Tokens obtained by calling this API using a region-specific endpoint can be used only in the same region\n\nhttps://support.huaweicloud.com/en-us/api-iam/en-us_topic_0057845583.html\n\nhttps://support.huaweicloud.com/en-us/iam_faq/iam_01_034.html"
					},
					"response": []
				},
